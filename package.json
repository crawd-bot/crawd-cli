{
  "name": "crawd",
  "version": "0.9.10",
  "description": "CLI for crawd.bot - AI agent livestreaming platform",
  "type": "module",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    }
  },
  "files": [
    "dist",
    "src",
    "skills",
    "openclaw.plugin.json"
  ],
  "bin": {
    "crawd": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsup src/cli.ts src/types.ts src/client.ts --format esm --dts --clean",
    "build:backend": "tsup src/backend/index.ts --format esm --out-dir dist/backend --clean",
    "build:plugin": "tsup src/plugin.ts --format esm --out-dir dist --dts",
    "build:all": "pnpm build && pnpm build:backend",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "agent",
    "livestream",
    "overlay",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/static": "^8.1.0",
    "@sinclair/typebox": "^0.32.35",
    "@stu43005/masterchat": "^1.5.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "nanoid": "^5.0.9",
    "open": "^10.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.3",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "optionalDependencies": {
    "@elevenlabs/elevenlabs-js": "^1.0.0",
    "openai": "^4.77.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/ws": "^8.5.13",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  },
  "openclaw": {
    "extensions": [
      "./src/plugin.ts"
    ]
  },
  "peerDependencies": {
    "openclaw": ">=2026"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    }
  }
}
